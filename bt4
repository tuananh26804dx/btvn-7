#include <stdio.h>


int main(){
	
	int m , n ;
	int i = 0;
	int j = 0;
	 
	printf("Hay nhap chieu dai m vao day :  ");
	scanf("%d" , &m);
	printf("Hay nhap chieu rong n vao day : ");
	scanf("%d", &n);
	
    for(  i = 0  ; i < m ; i++ ){
    	for(  j = 0  ; j < n ; j++){
    		printf("* "); 
		}
		printf("\n"); 
	}
        return 0;	 
} 
