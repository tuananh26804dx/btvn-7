#include <stdio.h>


int main(){
	
	int n;
	int i = 1; 
	long long gt = 1; 
	printf("Hay nhap so nguyen n vao day : ");
	scanf("%d",&n);
	
	if( n < 0 ){
		printf("Xin vui long nhap lai so nguyen nhe ban iu cua toi "); 
	} else {
		for( i ; i <= n ; i++){
			gt *= i ; 
		}
		
		printf("%d! = %lld ", n , gt ); 
	} 
	
	return 0; 
}
